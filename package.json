    "react-datepicker": "^2.10.1",
    "react-datetime": "^2.16.3",
    "react-dom": "16.8.6",
    "react-helmet": "6.0.0-beta",
    "react-highlight.js": "1.0.5",
    "react-intl": "2.8.0",
    "react-intl-redux": "^2.1.1",
    "react-keyboard-event-handler": "^1.5.4",
    "react-linkify": "^0.2.1",
    "react-materialize": "3.5.8",
    "react-redux": "^7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-saga": "1.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "4.0.0",
    "sanitize.css": "8.0.0",
    "styled-components": "4.2.0",
    "uikit-react": "^3.0.20",
    "ws": "^7.2.3"
