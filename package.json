    "immer": "3.0.0",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "materialize-css": "^1.0.0-rc.2",
    "minimist": ">=1.2.2",
    "moment": ">=2.19.3",
