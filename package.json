{
  "name": "onyx",
  "version": "0.1.0",
  "description": "Onyx AI",
  "repository": {
    "type": "git",
    "url": "git://github.com/OnyxAI/onyx.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Cassim Khouani",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "yarn analyze:clean",
    "analyze": "node ./scripts/analyze.js",
    "extract-intl": "node ./scripts/extract-intl.js",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production webpack --config scripts/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development node scripts/server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node scripts/server",
    "start:production": "yarn test && yarn build && yarn start:prod",
    "start:prod": "cross-env NODE_ENV=production node scripts/server",
    "clean:all": "yarn analyze:clean && yarn test:clean && yarn build:clean",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint src/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern scripts --ignore-pattern src/assets --ignore-pattern onyx",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern scripts --ignore-pattern src/assets --ignore-pattern onyx --fix",
    "lint:js": "yarn lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "yarn lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "axios": "^0.19.0",
    "chalk": "2.4.2",
    "compression": "1.7.4",
